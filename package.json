{
  "name": "storyblok-js-client",
  "version": "0.0.0-development",
  "description": "Universal JavaScript SDK for Storyblok's API",
  "types": "./types/index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "unpkg": "./dist/index.standalone.js",
  "jsdelivr": "./dist/index.standalone.js",
  "source": "source/index.js",
  "files": [
    "dist",
    "source",
    "types",
    "test"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "eslint --max-warnings=0 'source/**/*.{ts,js}'",
    "prettier": "prettier './**/*.js' './**/*.ts' './**/*.md' --write",
    "build": "run-s clean build:**",
    "build:es5": "ENABLE_BABEL=yes rollup --config",
    "build:es6": "ENABLE_BABEL=yes rollup --config",
    "build:standalone:es5": "STANDALONE=yes ENABLE_BABEL=yes rollup --config",
    "build:standalone:es6": "STANDALONE=yes ENABLE_BABEL=yes rollup --config",
    "test": "NODE_ENV='test' jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/storyblok-js-client.git"
  },
  "keywords": [
    "storyblok"
  ],
  "author": "Alexander Feiglstorfer <delooks@gmail.com>",
  "bugs": {
    "url": "https://github.com/storyblok/storyblok-js-client/issues"
  },
  "homepage": "https://github.com/storyblok/storyblok-js-client#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^26.3.0",
    "core-js": "^3.6.5",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-mjs-entry": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {},
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "ignorePatterns": "dist/",
    "rules": {
      "no-undef": 0,
      "no-async-promise-executor": 0
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  }
}
